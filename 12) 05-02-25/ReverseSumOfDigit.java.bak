import java.util.Scanner;
class ReverseSumOfDigit 
{
	public static void main(String[] args) 
	{
		System.out.print("Enter the number: ");
		int num = new Scanner(System.in).nextInt();
		int sum = 0;

		for (;num>0;)
		{
			int rem = num%10;
			sum+=rem;
			num/=10;
		}
		System.out.println(sum);
	}
}
