import java.util.Scanner;
class FindPrimeFactor
{
	public static void main(String[] args)
	{
		System.out.print("Enter a number: ");
		int num = new Scanner(System.in).nextInt();
		int temp=0;
		for (int i = 1;i<=num;i++)
		{
			if(num%i==0)
			{
				if(i%i==0){
					temp=1;
					//break;
				}
				else{
					System.out.println("prime factors :"+i);
					temp=0;
				}
			}
		}
	}
}